<span style="color:#000000; background-color:#C0C0C0;">会員情報編集</span>
<%= form_with model: @customer, url: public_customers_path, local:true do |f| %>
⽒名<%= f.text_field :last_name %><%= f.text_field :first_name %><br>
フリガナ<%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %><br>
郵便番号<%= f.text_field :postal_code %><br>
住所<%= f.text_field :address %><br>
電話番号<%= f.text_field :telephone_number %><br>
メールアドレス<%= f.email_field :email %><br>



<%= f.submit '編集内容を保存', class:"btn btn-success" %>
<%= link_to '退会する', public_customers_confirm_path, class:"btn btn-danger" %>

<% end %>





